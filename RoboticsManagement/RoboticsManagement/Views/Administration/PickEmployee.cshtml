@model IEnumerable<EmployeeTaskViewModel>

    @foreach (var employee in Model)
    {
        <form method="post" class="form-check"> @*Jakis opis specjalizacji powinien dodac wpisany ale to w AppUser*@
            <button type="submit" class="btn btn-outline-dark" asp-route-employeeId="@employee.EmployeeId" asp-route-taskId="@employee.TaskId">@employee.FirstName @employee.LastName</button>
        </form>
    }
<div class="col-12">
    <a class="btn btn-danger" asp-controller="Home" asp-action="Index">Cancel</a> @*Here can be alert with sth like "are you sure?"*@
</div>
