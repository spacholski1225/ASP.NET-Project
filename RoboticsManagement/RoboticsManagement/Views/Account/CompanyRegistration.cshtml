@model CompanyRegistartionViewModel
@{ 
    ViewData["Title"] = "Registration";
}
<form method="post">
    <div class="form-floating">
        <label asp-for="Email">Email</label>
        <input asp-for="Email" class="form-control" placeholder="Email" />
        <span asp-validation-for="Email" class="alert-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Password">Password</label>
        <input asp-for="Password" placeholder="Password" />
        <span asp-validation-for="Password" class="alert-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="ConfirmPassword">ConfirmPassword</label>
        <input asp-for="ConfirmPassword" placeholder="ConfirmPassword" />
        <span asp-validation-for="ConfirmPassword" class="alert-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="PhoneNumber">PhoneNumber</label>
        <input asp-for="PhoneNumber" placeholder="PhoneNumber" />
        <span asp-validation-for="PhoneNumber" class="alert-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="CompanyName">CompanyName</label>
        <input asp-for="CompanyName" placeholder="CompanyName" />
        <span asp-validation-for="CompanyName" class="alert-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="NIP">NIP</label>
        <input asp-for="NIP" placeholder="NIP" />
        <span asp-validation-for="NIP" class="alert-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Regon">Regon</label>
        <input asp-for="Regon" placeholder="Regon" />
        <span asp-validation-for="Regon" class="alert-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Country">Country</label>
        <input asp-for="Country" placeholder="Country" />
        <span asp-validation-for="Country" class="alert-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="City">City</label>
        <input asp-for="City" placeholder="City" />
        <span asp-validation-for="City" class="alert-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Adress">Adress</label>
        <input asp-for="Adress" placeholder="Adress" />
        <span asp-validation-for="Adress" class="alert-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="ZipCode">ZipCode</label>
        <input asp-for="ZipCode" placeholder="ZipCode" />
        <span asp-validation-for="ZipCode" class="alert-danger"></span>
    </div>
    <button type="submit" class="btn btn-success">Submit</button>
</form>